  "dependencies": {
    "@clerk/nextjs": "^6.12.6",
    "@nivo/core": "0.99.0",
    "@nivo/line": "0.99.0",
    "dayjs": "^1.11.13",
    "next": "15.2.2",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.6",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/nextjs": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/test": "8.6.14",
