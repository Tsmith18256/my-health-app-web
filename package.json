    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "9.27.0",
    "eslint-config-next": "15.2.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-storybook": "0.12.0",
    "husky": "9.1.7",
    "jsdom": "^26.1.0",
    "knip": "5.59.0",
    "lint-staged": "16.1.0",
    "postgres-shift": "^0.1.0",
    "prettier": "3.5.3",
    "storybook": "8.6.14",
    "typed-css-modules": "0.9.1",
    "typescript": "5.8.3",
